pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'I am building myself'
            }
        }
        stage('Test') {
            steps {
                echo 'I am testing the build'
            }
        }
        stage('Deploy') {
            steps {
                echo 'I am deploying the result'
            }
        }
        stage('Clear') {
            steps {
                echo 'I am cleaning myself'
            }
        }
    }
}
